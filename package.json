{
  "name": "svits",
  "homepage": "https://svits.vercel.app",
  "author": {
    "name": "Juan Villacorta",
    "email": "juanvillacortac@gmail.com",
    "url": "https://github.com/juandroid007"
  },
  "version": "0.0.0",
  "scripts": {
    "gulp:clean": "gulp clean",
    "gulp:dev": "gulp",
    "gulp:prod": "gulp prod",
    "validate": "svelte-check && tsc --noEmit",
    "dev": "run-s build:collections gulp:dev dev:app",
    "dev:app": "run-p cms-proxy dev:routify dev:svite",
    "dev:routify": "routify run",
    "dev:svite": "svite -ts",
    "cms-proxy": "netlify-cms-proxy-server",
    "build": "run-s build:collections build:routify build:svite gulp:prod",
    "build:routify": "routify run -b",
    "build:svite": "NODE_ENV=production svite build -ts",
    "build:collections": "node ./genCollections.js",
    "start": "sirv dist --host 0.0.0.0 --port 3000 --single",
    "prod": "run-s build start"
  },
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "@roxi/routify": "^2.7.4",
    "dayjs": "^1.9.7",
    "netlify-cms-app": "^2.14.4",
    "nprogress": "^0.2.0",
    "svelte": "3.29.4",
    "svelte-class-transition": "^1.0.4",
    "svelte-hmr": "0.11.1",
    "svelte-infinite-scroll": "^1.5.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@tailwindcss/ui": "^0.6.2",
    "@tsconfig/svelte": "^1.0.10",
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.10",
    "del": "^6.0.0",
    "favicons": "^6.2.0",
    "gray-matter": "^4.0.2",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-inject": "^5.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-string-replace": "^1.1.2",
    "gulp-webp": "^4.0.1",
    "mdsvex": "^0.8.9",
    "netlify-cms-proxy-server": "^1.3.7",
    "npm-run-all": "^4.1.5",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "rollup-plugin-svelte-svg": "^0.2.3",
    "sirv-cli": "^1.0.6",
    "sitemap": "^6.3.2",
    "svelte-check": "^1.0.53",
    "svelte-preprocess": "4.3.2",
    "svite": "^0.7.2",
    "tailwindcss": "^1.8.13",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3",
    "vercel": "^20.1.2"
  }
}
